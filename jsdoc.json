{
	"source": {
		"include": [
			"commands",
			"events",
			"handlers",
			"models",
			"modules"
		],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"recurse": true,
		"destination": "./docs/",
		"readme": "./readme.md",
		"template": "node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"theme": "dark",
			"title": "Pronto",
			"menu": [
				{
					"title": "GitHub Repository",
					"link": "https://github.com/JamesNZL/Pronto",
					"target": "_blank"
				}
			],
			"search": {
				"shouldSort": true,
				"threshold": 0.4,
				"location": 0,
				"distance": 100,
				"maxPatternLength": 32,
				"minMatchCharLength": 1
			},
			"overlay_scrollbar": {
				"option": {}
			}
		}
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"search": true
	}
}