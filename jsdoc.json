{
	"source": {
		"include": [
			"commands",
			"events",
			"handlers",
			"models",
			"modules"
		],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"recurse": true,
		"encoding": "utf8",
		"destination": "docs/",
		"readme": "README.md",
		"template": "node_modules/tidy-jsdoc",
		"prism-theme": "prism-okaidia"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"metadata": {
		"title": "Pronto"
	},
	"styles": {}
}