{
	"source": {
		"include": [
			"commands",
			"events",
			"handlers",
			"models",
			"modules"
		],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"recurse": true,
		"destination": "./docs/",
		"readme": "./readme.md"
	}
}