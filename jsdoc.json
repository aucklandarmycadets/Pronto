{
	"source": {
		"include": [
			"./"
		],
		"includePattern": ".js$",
		"excludePattern": "(node_modules|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"recurse": true,
		"encoding": "utf8",
		"destination": "docs/",
		"readme": "README.md",
		"template": "node_modules/tidy-jsdoc",
		"prism-theme": "prism-okaidia"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"staticFiles": {
				"include": [
					"docs/static/"
				]
			}
		},
		"css": [
			"styles/styles.css"
		]
	},
	"metadata": {
		"title": "Pronto"
	},
	"styles": {}
}