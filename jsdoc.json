{
	"source": {
		"include": [
			"./src"
		],
		"includePattern": ".js$",
		"excludePattern": "(node_modules|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"recurse": true,
		"encoding": "utf8",
		"destination": "docs/",
		"readme": "README.md",
		"template": "node_modules/tidy-jsdoc",
		"prism-theme": "prism-monokai"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"staticFiles": {
				"include": [
					"build/static/"
				]
			}
		},
		"css": []
	},
	"metadata": {
		"title": "Pronto",
		"logo": "static/logo.svg",
		"favicon": "static/favicon.svg"
	},
	"styles": {
		"primary-colour": "#ffd456",
		"secondary-colour": "#ffd456",
		"selection-text-colour": "#000"
	},
	"menu": [
		{
			"title": "GitHub Repository",
			"link": "https://github.com/JamesNZL/Pronto",
			"target": "_blank"
		}
	],
	"repository": {
		"link": "https://github.com/JamesNZL/Pronto",
		"branch": "major-refactor-v5",
		"type": "GitHub"
	}
}